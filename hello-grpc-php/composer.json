{
    "name": "feuyeux/hello-grpc",
    "description": "Hello gRPC for PHP",
    "require": {
        "grpc/grpc": "^1.57.0",
        "google/protobuf": "^v4.0.0",
        "ramsey/uuid": "^4.7.5",
        "monolog/monolog": "^3.5.0"
    },
    "autoload": {
        "psr-4": {
            "Hello\\": [
                "common/svc/Hello/",
                "common/msg/Hello/"
            ],
            "GPBMetadata\\": "common/msg/GPBMetadata/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}