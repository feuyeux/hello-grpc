{
    "name": "feuyeux/hello-grpc",
    "description": "Hello gRPC for PHP",
    "require": {
        "grpc/grpc": "^v1.57.0",
        "google/protobuf": "^v3.24.2",
        "ramsey/uuid": "^4.7",
        "apache/log4php": "2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Hello\\": [
                "common/svc/Hello/",
                "common/msg/Hello/"
            ],
            "GPBMetadata\\": "common/msg/GPBMetadata/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "10.3.5"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}